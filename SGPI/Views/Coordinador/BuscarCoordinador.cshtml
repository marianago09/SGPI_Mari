@{
    ViewData["Title"] = "Buscar coordinador.";
    Layout = "_LayoutCOORDINADOR";
}
<div class="container">
    <div class="row container">
        <div class="col-md-4">
            <h4>Buscar Usuario</h4>
        </div>
        <div class="col-md-6">
            <input type="text" class="form-control col-sm-6" id="Buscarusuariotxt" placeholder="Ingresar Usuario" name="Buscar Usuario">
        </div>

        <div class="col-md-2" style="
    position: relative;
    right: 13rem;
">
            <button type="button" class="btn btn-dark">Buscar Usuario</button>
        </div>
    </div>

    <table class="table table-bordered " style="margin-top: 3rem;">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Primer Apellido</th>
                <th>Segundo Apellido</th>
                <th>Genero</th>
                <th>Documento</th>
                <th>Programa</th>
                <th>Rol</th>
                <th>Correo Electronico</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>